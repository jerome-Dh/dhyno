{
    "name": "jeromedh/mvc-php",
    "type": "project",
	"version": "1.0",
    "description": "Mini architecture MVC en PHP",
	"author" : "Jerome Dh",
    "keywords": [
        "Bibliotheque",
        "MVC"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
   
    "autoload": {
        "psr-4": {
            "Core\\": "core/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
   
}